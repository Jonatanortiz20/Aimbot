			local TempGui = Instance.new("ScreenGui")
			print("Created Gui")
			TempGui.Name = "HideMe"
			TempGui.Parent = game:GetService("CoreGui")
			TempGui.Enabled = false
			local Hide_Or_Enable_Gui = function(args1)
				pcall(function()
					wait()
					if args1 == "Hide" then
						for i,v in ipairs(game:GetService("CoreGui"):GetChildren()) do
							wait()
							if v.DataCost == 4435 then
								v.Parent = TempGui
								wait()
								TempGui.Parent = game:GetService("ReplicatedStorage")
								wait()
							end
						end
					else
						for i,v in ipairs(game:GetService("ReplicatedStorage").HideMe:GetChildren()) do
							wait()
							if v.DataCost == 4435 then
								v.Parent = game:GetService("CoreGui")
								game:GetService("ReplicatedStorage").HideMe.Parent = game:GetService("CoreGui")
							end
						end
					end
				end)
			end
			function onChatted(msg, recipient, speaker)
				local source = string.lower(speaker.Name)
				msg = string.lower(msg)
				if string.sub(msg,1,9) == "/e enable" then
					_G.Recording = false;
					allesp = true
					_G.readyTo = true
					wait()
					if _G.FirstTime == true then
						for _,players in next,game:GetService("Players"):GetPlayers() do
							pcall(function()
								if players ~= game:GetService("Players").LocalPlayer then
									if players.Character:FindFirstChild("HumanoidRootPart") then
										players.Character.HumanoidRootPart.Transparency = 0
									else
										if players.Character:WaitForChild("HumanoidRootPart") then
											players.Character.HumanoidRootPart.Transparency = 0
										end
									end
								end
							end)
						end
					end
					Hide_Or_Enable_Gui("Enable")
				elseif string.sub(msg,1,10) == "/e disable" then
					_G.Recording = true
					allesp = false
					if _G.FirstTime == true then
						spawn(function()
							_G.readyTo = false
							while wait() do
								if _G.readyTo == true then
									break
								end
								for _,players in next,game:GetService("Players"):GetPlayers() do
									pcall(function()
										if players ~= game:GetService("Players").LocalPlayer then
											players.Character.HumanoidRootPart.Transparency = 1
										end
									end)
								end
							end
						end)
					end
					Hide_Or_Enable_Gui("Hide")
					wait()
				end
			end

			function onPlayerEntered(newPlayer)
				newPlayer.Chatted:connect(function(msg, recipient) onChatted(msg, recipient, newPlayer) end)
			end

			game.Players.ChildAdded:connect(onPlayerEntered)
			local p = game.Players:GetChildren()
			for i = 1, #p do
				p[i].Chatted:connect(function(msg, recipient) onChatted(msg, recipient, p[i]) end)
			end
		end)
	end)
