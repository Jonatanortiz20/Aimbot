				TextLabel.TextWrapped = true
				spawn(function()
					while wait(1) do
						Seconds = Seconds + 1
						if Seconds == 60 then
							Seconds = 0
							Minutes = Minutes + 1
						end
						if Minutes == 60 then
							Minutes = 0
							Hours = Hours + 1
						end
						TextLabel.Text = ("Time Played!: "..Hours.." H, "..Minutes.." M, "..Seconds.." S.")
					end
