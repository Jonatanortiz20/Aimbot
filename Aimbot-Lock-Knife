						pcall(function()
							OldPoints.Text = "+ "..Grab_Clan_Info(game:GetService("Players").LocalPlayer.Name,1)
							NewPoints.Text = "+ "..(Grab_Clan_Info(game:GetService("Players").LocalPlayer.Name,1) - PuntosActiguos)
						end)
					else
						Frame111.Visible = false
					end
				end
			end)

