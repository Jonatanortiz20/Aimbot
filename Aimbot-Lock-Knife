			local hbeexecuted = false
			local hbeeX = 4
			local hbeeY = 4
			local hbeeZ = 2
			local hbeeOn = false

			hbee:Toggle("Hitbox Extender",function(bap)
				hbeeOn = not bap
				wait()
				if hbeexecuted == true then
					if hbeeOn == true then
						for _,players in next,game:GetService("Players"):GetPlayers() do
							if players ~= game:GetService("Players").LocalPlayer then
								if players.Character:FindFirstChild("HumanoidRootPart") then
									players.Character.HumanoidRootPart.Size = Vector3.new(2, 2, 1)
									players.Character.HumanoidRootPart.Material = "ForceField"
									players.Character.HumanoidRootPart.Transparency = 1
								else
									if players.Character:WaitForChild("HumanoidRootPart") then
										players.Character.HumanoidRootPart.Size = Vector3.new(2, 2, 1)
										players.Character.HumanoidRootPart.Material = "ForceField"
										players.Character.HumanoidRootPart.Transparency = 1
									end
								end
							end
						end
					else
						for _,players in next,game:GetService("Players"):GetPlayers() do
							if players ~= game:GetService("Players").LocalPlayer then
								if players.Character:FindFirstChild("HumanoidRootPart") then
									players.Character.HumanoidRootPart.Size = Vector3.new(hbeeX, hbeeY, hbeeZ)
									players.Character.HumanoidRootPart.Material = "ForceField"
									if _G.Recording == true then
										players.Character.HumanoidRootPart.Transparency = 1
									else
										players.Character.HumanoidRootPart.Transparency = 0
									end
								else
									if players.Character:WaitForChild("HumanoidRootPart") then
										players.Character.HumanoidRootPart.Size = Vector3.new(hbeeX, hbeeY, hbeeZ)
										players.Character.HumanoidRootPart.Material = "ForceField"
										if _G.Recording == true then
										players.Character.HumanoidRootPart.Transparency = 1
									else
										players.Character.HumanoidRootPart.Transparency = 0
									end
									end
								end
							end
						end
					end
				end
			end)

			hbee:Slider("X Size",2,6,true,function(value)
				hbeeX = value
			end)

			hbee:Slider("Y Size",2,6,true,function(value)
				hbeeY = value
			end)

			hbee:Slider("Z Size",1,3,true,function(value)
				hbeeZ = value
			end)

			local FileName = "Ai Assist.txt"
			local HttpService = game:GetService("HttpService")
			local SaveSettings = function(Text)
				writefile(FileName, HttpService:JSONEncode(Text))
				print("Saved Setting's!")
			end
			local SavedSettings;
			if isfile(FileName) then
				SavedSettings = HttpService:JSONDecode(readfile(FileName))
			else
				warn("Settings File Not Found!")
			end

			hbee:Button("Hbe",function()
				hbeexecuted = true
				_G.FirstTime = true
				spawn(function()
					local TORSOSIZEEEE = Vector3.new(game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Size.X,game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Size.Y,game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Size.Z)
					local gmt = getrawmetatable(game)
					local oldindex = gmt.__index

					if setreadonly then
						setreadonly(gmt, false)
					else
						make_writeable(gmt, true)
					end

					gmt.__index = newcclosure(function(t, k)
						if type(t) == "userdata" and oldindex(t, "Name") == "HumanoidRootPart" and k == "Size" then
							return TORSOSIZEEEE
						end
						return oldindex(t, k)
					end)

					if setreadonly then
						setreadonly(gmt, true)
					else
						make_writeable(gmt, false)
					end
					wait(2)
					local ToDoAfterCharacterFound = function(Character)
						local ElHumanoid = Character:WaitForChild("HumanoidRootPart")
						while Character and ElHumanoid do
							wait(1.5)
							if Character and ElHumanoid then
								break
							end
						end
						Character.HumanoidRootPart.Size = Vector3.new(hbeeX, hbeeY, hbeeZ)
						Character.HumanoidRootPart.Material = "ForceField"
						Character.HumanoidRootPart.Transparency = 0
					end
					for _,players in next,game:GetService("Players"):GetPlayers() do
						if players ~= game:GetService("Players").LocalPlayer then
							if players.Character:FindFirstChild("HumanoidRootPart") then
								players.Character.HumanoidRootPart.Size = Vector3.new(hbeeX, hbeeY, hbeeZ)
								players.Character.HumanoidRootPart.Material = "ForceField"
								if _G.Recording == true then
										players.Character.HumanoidRootPart.Transparency = 1
									else
										players.Character.HumanoidRootPart.Transparency = 0
									end
								players.CharacterAdded:Connect(function(charr)
									ToDoAfterCharacterFound(charr)
								end)
							else
								if players.Character:WaitForChild("HumanoidRootPart") then
									players.Character.HumanoidRootPart.Size = Vector3.new(hbeeX, hbeeY, hbeeZ)
									players.Character.HumanoidRootPart.Material = "ForceField"
									if _G.Recording == true then
										players.Character.HumanoidRootPart.Transparency = 1
									else
										players.Character.HumanoidRootPart.Transparency = 0
									end
									players.CharacterAdded:Connect(function(charr)
										ToDoAfterCharacterFound(charr)
									end)
								end
							end
						end
					end
					game:GetService("Players").PlayerAdded:Connect(function(plr)
						plr.CharacterAdded:Connect(function(chur)
							ToDoAfterCharacterFound(chur)
						end)
					end)
				end)
			end)

			hbee:Button("Save Settings!",function()
				print("Saving Settings..!")
				if hbeexecuted == true then
					local Configuraciones = {
						hbeeX = hbeeX,
						hbeeY = hbeeY,
						hbeeZ = hbeeZ,
						hbeeOn = hbeeOn,
						hbeexecuted = hbeexecuted
					}
					SaveSettings(Configuraciones)
				else
					local Configuraciones = {
						hbeeX = hbeeX,
						hbeeY = hbeeY,
						hbeeZ = hbeeZ,
						hbeeOn = hbeeOn
					}
					SaveSettings(Configuraciones)
				end
			end)

			hbee:Button("Load Settings!",function()
				print("Loading Settigns...")
				hbeeX = SavedSettings.hbeeX
				hbeeY = SavedSettings.hbeeY
				hbeeZ = SavedSettings.hbeeZ
				hbeeOn = SavedSettings.hbeeOn
				if SavedSettings.hbeexecuted == true then
					spawn(function()
						local TORSOSIZEEEE = Vector3.new(game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Size.X,game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Size.Y,game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Size.Z)
						local gmt = getrawmetatable(game)
						local oldindex = gmt.__index

						if setreadonly then
							setreadonly(gmt, false)
						else
							make_writeable(gmt, true)
						end

						gmt.__index = newcclosure(function(t, k)
							if type(t) == "userdata" and oldindex(t, "Name") == "HumanoidRootPart" and k == "Size" then
								return TORSOSIZEEEE
							end
							return oldindex(t, k)
						end)

						if setreadonly then
							setreadonly(gmt, true)
						else
							make_writeable(gmt, false)
						end
						wait(2)
						hbeexecuted = true
						local ToDoAfterCharacterFound = function(Character)
							local ElHumanoid = Character:WaitForChild("HumanoidRootPart")
							while Character and ElHumanoid do
								wait(1.5)
								if Character and ElHumanoid then
									break
								end
							end
							Character.HumanoidRootPart.Size = Vector3.new(hbeeX, hbeeY, hbeeZ)
							Character.HumanoidRootPart.Material = "ForceField"
							Character.HumanoidRootPart.Transparency = 0
						end
						for _,players in next,game:GetService("Players"):GetPlayers() do
							if players ~= game:GetService("Players").LocalPlayer then
								if players.Character:FindFirstChild("HumanoidRootPart") then
									players.Character.HumanoidRootPart.Size = Vector3.new(hbeeX, hbeeY, hbeeZ)
									players.Character.HumanoidRootPart.Material = "ForceField"
									if _G.Recording == true then
										players.Character.HumanoidRootPart.Transparency = 1
									else
										players.Character.HumanoidRootPart.Transparency = 0
									end
									players.CharacterAdded:Connect(function(charr)
										ToDoAfterCharacterFound(charr)
									end)
								else
									if players.Character:WaitForChild("HumanoidRootPart") then
										players.Character.HumanoidRootPart.Size = Vector3.new(hbeeX, hbeeY, hbeeZ)
										players.Character.HumanoidRootPart.Material = "ForceField"
										if _G.Recording == true then
										players.Character.HumanoidRootPart.Transparency = 1
									else
										players.Character.HumanoidRootPart.Transparency = 0
									end
										players.CharacterAdded:Connect(function(charr)
											ToDoAfterCharacterFound(charr)
										end)
									end
								end
							end
						end
						game:GetService("Players").PlayerAdded:Connect(function(plr)
							plr.CharacterAdded:Connect(function(chur)
								ToDoAfterCharacterFound(chur)
							end)
						end)
					end)
				end
			end)

			espsettings:Toggle("Esp",function(bap)
				allesp = bap
			end)

			local EspToggleBind = "t"

			espsettings:Box("Esp Toggle","string",function(brrr)
				EspToggleBind =  brrr
				wait()
			end)

			Mouse.KeyDown:Connect(function(TheKeyPressed)
				if TheKeyPressed == (string.lower(EspToggleBind) or string.upper(EspToggleBind)) then
					if allesp == true then
						allesp = false
					else
						allesp = true
					end
				end
			end)

			espsettings:Toggle("Tracers",function(bap)
				tracers = bap
			end)

			espsettings:Toggle("Boxes",function(bap)
				CreateTextboxes = bap
			end)

			espsettings:Toggle("Names",function(bap)
				names = bap
			end)

			espsettings:ColorPicker("ESP Color",Color3.fromRGB(53, 220, 53),function(color)
				allespcolor = color
			end)

			espsettings:Toggle("TeamCheck",function(Okayf)
				ESPTeamCheck = Okayf
			end)

			espsettings:Toggle("TeamColor",function(balp)
				ESPTeamColor = balp
			end)

			local ESPText = function(part)
				local name = Drawing.new("Text")
				name.Text = part.Parent.Name
				name.Position = ESPPOS(part)
				name.Size = 20.00
				name.Outline = true
				name.Center = true
				name.Visible = true
				local Stepped;
				local ReadyToDisc = false
				Stepped = game:GetService("RunService").Heartbeat:Connect(function()
					if name ~= nil then
						if part:IsDescendantOf(game:GetService("Workspace")) then
							if allesp == true then
								if _G.Recording == false then
									if names == true then
										name.Position = ESPPOS(part)
										local _, screen = Work.CurrentCamera:WorldToViewportPoint(part.Position)
										if screen then
											if localPlayer:DistanceFromCharacter(part.Position) <= 1200 then
												name.Visible = true
											else
												name.Visible = false
											end
										else
											name.Visible = false
										end
										if allespcolor then
											if MyTarget() and part.Parent.Name == MyTarget().Name then
												name.Color = targetespcolor
											else
												name.Color = allespcolor
											end
										end
									else
										name.Visible = false
									end
								else
									name.Visible = false
								end
							else
								name.Visible = false
							end
						else
							name.Visible = false
							ReadyToDisc = true
						end
					else
						name.Visible = false
						ReadyToDisc = true
					end
				end)
				local readytoleave = false
				spawn(function()
					while wait() do
						if ReadyToDisc == true then
							name:Remove()
							Stepped:Disconnect()
							readytoleave = true
						end
						if readytoleave == true then
							break
						end
					end
				end)
			end

			local ESPLine = function(part)
				local Line = Drawing.new("Line")
				Line.Visible = true
				Line.From = Vector2.new(1000, 15)
				Line.To = ESPPOS(part)
				Line.Thickness = 2
				Line.Transparency = 1
				local Stepped
				local ReadyToDisc = false
				Stepped = game:GetService("RunService").Heartbeat:Connect(function()
					if Line ~= nil then
						if part:IsDescendantOf(game:GetService("Workspace")) then
							if allesp == true then
								if _G.Recording == false then
									if tracers == true then
										Line.To = ESPPOS(part)
										local _, screen = Work.CurrentCamera:WorldToViewportPoint(part.Position)
										if screen then
											if localPlayer:DistanceFromCharacter(part.Position) <= 1200 then
												Line.Visible = true
											else
												Line.Visible = false
											end
										else
											Line.Visible = false
										end
										if allespcolor then
											if MyTarget() and part.Parent.Name == MyTarget().Name then
												Line.Color = targetespcolor
											else
												Line.Color = allespcolor
											end
										end
									else
										Line.Visible = false
									end
								else
									Line.Visible = false
								end
							else
								Line.Visible = false
							end
						else
							Line.Visible = false
							ReadyToDisc = true
						end
					else
						Line.Visible = false
						ReadyToDisc = true
					end
				end)
				local readytoleave = false
				spawn(function()
					while wait() do
						if ReadyToDisc == true then
							Line:Remove()
							Stepped:Disconnect()
							readytoleave = true
						end
						if readytoleave == true then
							break
						end
					end
				end)
			end
			local plr = game.Players.LocalPlayer
			local camera = game.Workspace.CurrentCamera
			local ESPSquare = function(part,part2)
				local Top = Drawing.new("Line")
				Top.Visible = false
				Top.From = Vector2.new(0, 0)
				Top.To = Vector2.new(200, 200)
				Top.Thickness = 2
				Top.Transparency = 1

				local Bottom = Drawing.new("Line")
				Bottom.Visible = false
				Bottom.From = Vector2.new(0, 0)
				Bottom.To = Vector2.new(200, 200)
				Bottom.Thickness = 2
				Bottom.Transparency = 1

				local Left = Drawing.new("Line")
				Left.Visible = false
				Left.From = Vector2.new(0, 0)
				Left.To = Vector2.new(200, 200)
				Left.Color = Color3.fromRGB(255, 0, 0)
				Left.Thickness = 2
				Left.Transparency = 1

				local Right = Drawing.new("Line")
				Right.Visible = false
				Right.From = Vector2.new(0, 0)
				Right.To = Vector2.new(200, 200)
				Right.Thickness = 2
				Right.Transparency = 1
				local ReadyToDisc = false
				local connection;
				wait()
				connection = game:GetService("RunService").Heartbeat:Connect(function()
					if Top ~= nil and Left ~= nil and Bottom ~= nil and Right ~= nil then 
						if part2:IsDescendantOf(game:GetService("Workspace")) then
							if allesp == true then
								if _G.Recording == false then
									if CreateTextboxes == true then
										local ScreenPos, OnScreen = camera:WorldToViewportPoint(part2.Position)
										if OnScreen then
											local Scale = part.Size.Y / 2
											local Size = Vector3.new(2, 3, 0) * (Scale * 2)
											local humpos = camera:WorldToViewportPoint(part2.Position)
											local TL = camera:WorldToViewportPoint((part2.CFrame * CFrame.new(Size.X, Size.Y, 0)).p)
											local TR = camera:WorldToViewportPoint((part2.CFrame * CFrame.new(-Size.X, Size.Y, 0)).p)
											local BL = camera:WorldToViewportPoint((part2.CFrame * CFrame.new(Size.X, -Size.Y, 0)).p)
											local BR = camera:WorldToViewportPoint((part2.CFrame * CFrame.new(-Size.X, -Size.Y, 0)).p)
											Top.From = Vector2.new(TL.X, TL.Y)
											Top.To = Vector2.new(TR.X, TR.Y)
											Left.From = Vector2.new(TL.X, TL.Y)
											Left.To = Vector2.new(BL.X, BL.Y)
											Right.From = Vector2.new(TR.X, TR.Y)
											Right.To = Vector2.new(BR.X, BR.Y)
											Bottom.From = Vector2.new(BL.X, BL.Y)
											Bottom.To = Vector2.new(BR.X, BR.Y)
											if localPlayer:DistanceFromCharacter(part2.Position) <= 1200 then
												Top.Visible = true
												Left.Visible = true
												Bottom.Visible = true
												Right.Visible = true
											else
												Top.Visible = false
												Left.Visible = false
												Bottom.Visible = false
												Right.Visible = false
											end
											if allespcolor then
												if MyTarget() and part.Parent.Name == MyTarget().Name then
													Top.Color = targetespcolor
													Left.Color = targetespcolor
													Bottom.Color = targetespcolor
													Right.Color = targetespcolor
												else
													Top.Color = allespcolor
													Left.Color = allespcolor
													Bottom.Color = allespcolor
													Right.Color = allespcolor
												end
											end
										else
											Top.Visible = false
											Left.Visible = false
											Bottom.Visible = false
											Right.Visible = false
										end
									else
										Top.Visible = false
										Left.Visible = false
										Bottom.Visible = false
										Right.Visible = false
									end
								else
									Top.Visible = false
									Left.Visible = false
									Bottom.Visible = false
									Right.Visible = false
								end
							else
								Top.Visible = false
								Left.Visible = false
								Bottom.Visible = false
								Right.Visible = false
							end
						else
							Top.Visible = false
							Left.Visible = false
							Bottom.Visible = false
							Right.Visible = false
							ReadyToDisc = true
						end
					else
						Top.Visible = false
						Left.Visible = false
						Bottom.Visible = false
						Right.Visible = false
						ReadyToDisc = true
					end
				end)
				local readytoleave = false
				spawn(function()
					while wait() do
						if ReadyToDisc == true then
							Top:Remove()
							Left:Remove()
							Bottom:Remove()
							Right:Remove()
							connection:Disconnect()
							readytoleave = true
						end
						if readytoleave == true then
							break
						end
					end
				end)
			end

			espsettings:Button("ALL",function()
				local ToDoAfterCharacterFound = function(Character)
					local ElHumanoid = Character:WaitForChild("HumanoidRootPart")
					local ElHead = Character:WaitForChild("Head")
					while Character and ElHumanoid and ElHead do
						wait(1.5)
						if Character and ElHumanoid and ElHead then
							break
						end
					end
					ESPText(Character.Head)
					ESPLine(Character.Head)
					ESPSquare(Character.Head,Character.HumanoidRootPart)
				end
				for _,players in next,game:GetService("Players"):GetPlayers() do
					if players ~= safe.Players.LocalPlayer then
						if players.Character:FindFirstChild("Humanoid") then
							ESPText(players.Character.Head)
							ESPLine(players.Character.Head)
							ESPSquare(players.Character.Head,players.Character.HumanoidRootPart)
							players.CharacterAdded:Connect(function(charr)
								ToDoAfterCharacterFound(charr)
							end)
						else
							if players.Character:WaitForChild("Humanoid") then
								ESPText(players.Character.Head)
								ESPLine(players.Character.Head)
								ESPSquare(players.Character.Head,players.Character.HumanoidRootPart)
								players.CharacterAdded:Connect(function(charr)
									ToDoAfterCharacterFound(charr)
								end)
							end
						end
					end
				end
				game:GetService("Players").PlayerAdded:Connect(function(plr)
					plr.CharacterAdded:Connect(function(chur)
						ToDoAfterCharacterFound(chur)
					end)
				end)
			end)

			espsettings:Button("Tracers",function()
				local ToDoAfterCharacterFound = function(Character)
					local ElHumanoid = Character:WaitForChild("HumanoidRootPart")
					local ElHead = Character:WaitForChild("Head")
					while Character and ElHumanoid and ElHead do
						wait(1.5)
						if Character and ElHumanoid and ElHead then
							break
						end
					end
					pcall(function()
						ESPLine(Character.Head)
					end)
				end
				for _,players in next,game:GetService("Players"):GetPlayers() do
					pcall(function()
						if players ~= safe.Players.LocalPlayer then
							if players.Character:FindFirstChild("Humanoid") then
								ESPLine(players.Character.Head)
								players.CharacterAdded:Connect(function(charr)
									ToDoAfterCharacterFound(charr)
								end)
							else
								if players.Character:WaitForChild("Humanoid") then
									ESPLine(players.Character.Head)
									players.CharacterAdded:Connect(function(charr)
										ToDoAfterCharacterFound(charr)
									end)
								end
							end
						end
					end)
				end
				game:GetService("Players").PlayerAdded:Connect(function(plr)
					plr.CharacterAdded:Connect(function(chur)
						ToDoAfterCharacterFound(chur)
					end)
				end)
			end)

			espsettings:Button("Boxes",function()
				local ToDoAfterCharacterFound = function(Character)
					local ElHumanoid = Character:WaitForChild("HumanoidRootPart")
					local ElHead = Character:WaitForChild("Head")
					while Character and ElHumanoid and ElHead do
						wait(1.5)
						if Character and ElHumanoid and ElHead then
							break
						end
					end
					pcall(function()
						ESPSquare(Character.Head,Character.HumanoidRootPart)
					end)
				end
				for _,players in next,game:GetService("Players"):GetPlayers() do
					if players ~= safe.Players.LocalPlayer then
						pcall(function()
							if players.Character:FindFirstChild("Humanoid") then
								ESPSquare(players.Character.Head,players.Character.HumanoidRootPart)
								players.CharacterAdded:Connect(function(charr)
									ToDoAfterCharacterFound(charr)
								end)
							else
								if players.Character:WaitForChild("Humanoid") then
									ESPSquare(players.Character.Head,players.Character.HumanoidRootPart)
									players.CharacterAdded:Connect(function(charr)
										ToDoAfterCharacterFound(charr)
									end)
								end
							end
						end)
					end
				end
				game:GetService("Players").PlayerAdded:Connect(function(plr)
					plr.CharacterAdded:Connect(function(chur)
						ToDoAfterCharacterFound(chur)
					end)
				end)
			end)

			espsettings:Button("Names",function()
				local ToDoAfterCharacterFound = function(Character)
					local ElHumanoid = Character:WaitForChild("HumanoidRootPart")
					local ElHead = Character:WaitForChild("Head")
					while Character and ElHumanoid and ElHead do
						wait(1.5)
						if Character and ElHumanoid and ElHead then
							break
						end
					end
					ESPText(Character.Head)
				end
				for _,players in next,game:GetService("Players"):GetPlayers() do
					if players ~= safe.Players.LocalPlayer then
						if players.Character:FindFirstChild("Humanoid") then
							ESPText(players.Character.Head)
							players.CharacterAdded:Connect(function(charr)
								ToDoAfterCharacterFound(charr)
							end)
						else
							if players.Character:WaitForChild("Humanoid") then
								ESPText(players.Character.Head)
								players.CharacterAdded:Connect(function(charr)
									ToDoAfterCharacterFound(charr)
								end)
							end
						end
					end
				end
				game:GetService("Players").PlayerAdded:Connect(function(plr)
					plr.CharacterAdded:Connect(function(chur)
						ToDoAfterCharacterFound(chur)
					end)
				end)
			end)

			UserInputService.InputBegan:Connect(function(Input)
				if Input.UserInputType == Enum.UserInputType.MouseButton2 then
					if aimbotkey == "Right Mouse" then
						Update()
						aimboton = true
					end
				end
			end)

			UserInputService.InputEnded:Connect(function(Input)
				if Input.UserInputType == Enum.UserInputType.MouseButton2 then
					if aimbotkey == "Right Mouse" then
						aimboton = false
					end
				end
			end)

			UserInputService.InputBegan:Connect(function(Input)
				if Input.UserInputType == Enum.UserInputType.MouseButton1 then
					if aimbotkey == "Left Mouse" then
						Update()
						aimboton = true
					end
				end
			end)

			UserInputService.InputEnded:Connect(function(Input)
				if Input.UserInputType == Enum.UserInputType.MouseButton1 then
					if aimbotkey == "Left Mouse" then
						aimboton = false
					end
				end
			end)

