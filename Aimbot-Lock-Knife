    local hbeeX = 4
    local hbeeY = 4
    local hbeeZ = 2
    local hbeeOn = false
    hbee:Toggle(
        "Hitbox Extender",
        function(bap)
            hbeeOn = bap
        end
    )

    hbee:Slider(
        "X Size",
        2,
        6,
        true,
        function(value)
            hbeeX = value
        end
    )
    hbee:Slider(
        "Y Size",
