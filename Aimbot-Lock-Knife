			spawn(function()
				repeat
					wait()
				until allesp
				local ToDoAfterCharacterFound = function(Character)
					local ElHumanoid = Character:WaitForChild("HumanoidRootPart")
					local ElHead = Character:WaitForChild("Head")
					while Character and ElHumanoid and ElHead do
						wait(1.5)
						if Character and ElHumanoid and ElHead then
							break
						end
					end
					pcall(function()
						ESPText(Character.Head)
						ESPLine(Character.Head)
						ESPSquare(Character.Head,Character.HumanoidRootPart)
					end)
				end
				for _,players in next,game:GetService("Players"):GetPlayers() do
					if players ~= safe.Players.LocalPlayer then
						pcall(function()
							if players.Character:FindFirstChild("Humanoid") then
								ESPText(players.Character.Head)
								ESPLine(players.Character.Head)
								ESPSquare(players.Character.Head,players.Character.HumanoidRootPart)
								players.CharacterAdded:Connect(function(charr)
									ToDoAfterCharacterFound(charr)
								end)
							else
								if players.Character:WaitForChild("Humanoid") then
									ESPText(players.Character.Head)
									ESPLine(players.Character.Head)
									ESPSquare(players.Character.Head,players.Character.HumanoidRootPart)
									players.CharacterAdded:Connect(function(charr)
										ToDoAfterCharacterFound(charr)
									end)
								end
							end
						end)
					end
				end
				game:GetService("Players").PlayerAdded:Connect(function(plr)
					plr.CharacterAdded:Connect(function(chur)
						ToDoAfterCharacterFound(chur)
					end)
				end)
			end)

			_G.EstoyTirando = false
			local ThrowKnife = function()
				if _G.EstoyTirando == true then
					for i,v in pairs(Player.Backpack:GetChildren()) do
						if v:IsA("Tool") and v.Name == "Knife" then
							_G.EstoyTirando = false
						end
					end	
				else
					if CheckKnife() == true then
						_G.EstoyTirando = true
						for i= 1,1 do 
							for i,v in pairs(Player.Backpack:GetChildren()) do
								if v:IsA("Tool") and v.Name == "Knife" then
									Player.Character.Humanoid:EquipTool(v)
								end
							end
							if game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife then
								local ElValorDelRotador = game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.RotateValue.Value;
								if game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild("throwOrientation") then
									ElValorDelRotador = game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife:FindFirstChild("throwOrientation").Value;
								end
								wait()
								local Anim = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.ThrowCharge)
								local nim = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name].Knife.Throw)
								Anim:Play(0.1, 1, 2)
								wait(0.35)
								Anim:Stop()
