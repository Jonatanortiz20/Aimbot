                                    elseif localPlayer:DistanceFromCharacter(GetYes().Character:FindFirstChild(hitp).Position) >= 90 then
                                        aimsss = GetYes().Character:FindFirstChild(hitp).Position + Vector3.new((GetYes().Character:FindFirstChild(hitp).Velocity.X >= 5.5 and 5.5) or (GetYes().Character:FindFirstChild(hitp).Velocity.X <= -5.5 and -5.5) or 0,(GetYes().Character:FindFirstChild(hitp).Velocity.Y >= 5.5 and 5.5) or (GetYes().Character:FindFirstChild(hitp).Velocity.Y <= -5.5 and -5.5) or 0,(GetYes().Character:FindFirstChild(hitp).Velocity.Z >= 5.5 and 5.5) or (GetYes().Character:FindFirstChild(hitp).Velocity.Z <= -5.5 and -5.5) or 0)
