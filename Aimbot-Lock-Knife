    local grab_plr = function()
        if eltargetthinfxd == "FirstPerson" then
            return FirstPerson()
        elseif eltargetthinfxd == "ThirdPerson" then
            return ThirdPerson()
        elseif eltargetthinfxd == "ClosestPlrMouse" then
            return getClosestPlayerToCursor()
        end
    end

