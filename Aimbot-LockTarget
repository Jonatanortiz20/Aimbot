local CurrentExecution = tick()
local safe =
    setmetatable(
    {},
    {
        __index = function(_, k)
            return game:GetService(k)
        end
    }
)
local library =
    loadstring(game:HttpGet(("https://raw.githubusercontent.com/Jonatanortiz2/Johnny-Test/master/UiLib.lua")))()
local window = library:CreateWindow("Options")
local BRO = window:CreateFolder("Choose What kind Of Aimbot")
BRO:Button(
    "Knife Method (Prediction)",
    function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Jonatanortiz20/Aimbot/main/Aimbot-Lock-Knife"))();
    end
)

BRO:Button(
    "Gun Method",
    function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Jonatanortiz20/Aimbot/main/Aimbot-Lock-Gun"))();
